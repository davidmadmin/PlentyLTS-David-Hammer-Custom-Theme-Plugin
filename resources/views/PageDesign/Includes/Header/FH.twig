            <span class="fh-header__basket-count" v-basket-item-quantity="$store.state.basket.data.itemQuantity">{{ basket.itemQuantity | default(0) }}</span>
          </template>
          <template v-else>
            <span class="fh-header__basket-count">{{ basket.itemQuantity | default(0) }}</span>
          </template>
